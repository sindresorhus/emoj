    "neural",
    "networks"
  ],
  "dependencies": {
    "auto-bind": "^1.1.0",
    "clipboardy": "^0.1.1",
    "conf": "^1.0.0",
    "got": "^6.3.0",
    "has-ansi": "^2.0.0",
    "import-jsx": "^1.0.0",
    "ink": "github:vadimdemedes/ink",
    "lodash.debounce": "^4.0.6",
    "mem": "^1.1.0",
    "meow": "^3.7.0",
    "skin-tone": "^1.0.0"
  },
  "devDependencies": {
    "ava": "*",
    "eslint-config-xo-react": "^0.13.0",
    "eslint-plugin-react": "^7.1.0",
    "xo": "^0.18.2"
  },
  "xo": {
    "extends": [
      "xo-react"

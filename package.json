{
	"name": "emoj",
	"version": "3.3.0",
	"description": "Find relevant emoji from text on the command-line",
	"license": "MIT",
	"repository": "sindresorhus/emoj",
	"funding": "https://github.com/sponsors/sindresorhus",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"

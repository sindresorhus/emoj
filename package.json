{
  "name": "emoj",
  "version": "1.1.0",
  "description": "Find relevant emoji from text on the command-line",
  "license": "MIT",
  "repository": "sindresorhus/emoj",
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "sindresorhus.com"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "xo && ava"

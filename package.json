		"skin-tone": "^3.0.0",
		"unicode-emoji-json": "^0.4.0"
	},
	"devDependencies": {
		"ava": "^5.3.1",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"xo": "^0.56.0"
	},
	"xo": {
